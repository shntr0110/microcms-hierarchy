{
  "name": "dreary-dwarf",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=20.12.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "biome check src",
    "lint:fix": "biome check --apply src",
    "format": "biome format src",
    "format:fix": "biome format --write src",
    "format:prettier": "prettier \"src/**/*.astro\" --check",
    "format:prettier:fix": "prettier \"src/**/*.astro\" --write",
    "lint:all": "npm run lint && npm run lint:markup && npm run format && npm run format:prettier && npm run lint:style",
    "lint:allfix": "npm run lint:fix && npm run lint:markup && npm run format:fix && npm run format:prettier:fix && npm run lint:style:fix",
    "lint:style": "stylelint './src/**/*.{scss,css}'",
    "lint:style:fix": "npm run lint:style -- --fix",
    "lint:markup": "markuplint \"src/**/*.{astro,jsx,tsx}\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^3.6.0",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "astro": "^4.11.3",
    "astro-google-fonts-optimizer": "^0.2.2",
    "astro-seo": "^0.8.4",
    "clsx": "^2.1.1",
    "lucide-react": "^0.399.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.57.0",
    "react-phone-number-input": "^3.4.12",
    "typescript": "^5.5.2",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^3.1.6",
    "@biomejs/biome": "1.9.4",
    "@markuplint/astro-parser": "^4.6.5",
    "@markuplint/jsx-parser": "^4.7.5",
    "@markuplint/ml-config": "^4.7.2",
    "@markuplint/react-spec": "^4.5.5",
    "@types/react": "^18.3.3",
    "globals": "^15.6.0",
    "markuplint": "^4.9.2",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.97.1",
    "stylelint": "^16.6.1",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-plugin-logical-css": "^1.2.3",
    "taks-stylelint-order": "^1.1.2"
  }
}
